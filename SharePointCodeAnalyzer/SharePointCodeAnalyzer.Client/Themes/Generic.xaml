                            <Image Width="24"
                                   Height="24"
                                   Margin="10"
                                   HorizontalAlignment="Left"
                                   VerticalAlignment="Top"
                                   Opacity="0.5"
                                   Source="/Images\shared\images\icons/locked.png"
                                   ToolTipService.ToolTip="{Binding LockedReasonInformation}"
                                   Visibility="{Binding IsLocked,
                                                        Converter={StaticResource BooleanToVisibilityConverter}}" />
                        </Grid>
                    </Border>

                </ControlTemplate>
            </Setter.Value>
        </Setter>
