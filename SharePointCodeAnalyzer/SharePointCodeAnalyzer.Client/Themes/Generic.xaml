                    </Grid.Background>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto" />
                        <RowDefinition Height="95" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="0">
                        <Border Style="{StaticResource StatusBarBorderStyle}" core:DynamicStyleManager.SnappedStyle="{StaticResource StatusBarBorderStyle_Snapped}">
                            <controls:FadingItemsControl xmlns:p11="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
