    <DataTemplate x:Key="StatusBarComponentItemTemplate">
        <Border Margin="15 0 0 0" core:StateManager.CurrentState="{Binding IsVisibleState}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="AnimationStates">
                    <VisualState x:Name="True">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="content"
                                             Storyboard.TargetProperty="(FrameworkElement.MaxWidth)"
                                             To="250">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseInOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="content"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="False">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="content"
                                             Storyboard.TargetProperty="(FrameworkElement.MaxWidth)"
                                             To="0">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseInOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="content"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="content" Opacity="1">
                <ContentControl VerticalAlignment="Stretch"
                                HorizontalContentAlignment="Right"
                                VerticalContentAlignment="Stretch"
                                Content="{Binding}"
                                ContentTemplate="{Binding ConverterParameter=StatusBarTemplate,
                                                          Converter={StaticResource DataTemplateConverter}}"
                                DataContext="{Binding}" />
            </Grid>
        </Border>
    </DataTemplate>

