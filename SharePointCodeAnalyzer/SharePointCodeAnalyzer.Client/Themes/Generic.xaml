    <Style xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           x:Key="{x:Type controls:LoadingPanel}"
           TargetType="{x:Type controls:LoadingPanel}">
        <Setter Property="HorizontalAlignment" Value="Stretch" />
        <Setter Property="VerticalAlignment" Value="Stretch" />
        <Setter Property="BorderThickness" Value="2" />
        <Setter Property="Background" Value="#00FFFFFF" />
        <Setter Property="BorderBrush" Value="#FF000000" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type controls:LoadingPanel}">
                    <Grid>
                        <Border HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Background="{StaticResource LoadingPanelFullScreenBackground}"
                                Visibility="{TemplateBinding IsBusyVisibility}">
                            <Border Width="400"
                                    MinHeight="150"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Background="{StaticResource LoadingPanelBackground}"
                                    BorderBrush="#FF000000"
                                    BorderThickness="3">
                                <Grid Margin="20 10 20 10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="50" />
                                    </Grid.RowDefinitions>
                                    <StackPanel Grid.Row="0"
                                                HorizontalAlignment="Left"
                                                VerticalAlignment="Center"
                                                Orientation="Horizontal">
                                        <controls:ProgressRing xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
                                                               xmlns:p12="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                               Width="24"
                                                               Height="24"
                                                               Margin="20 0 20 0"
                                                               IsActive="{p12:TemplateBinding IsBusy}" />
                                        <TextBlock xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
                                                   Style="{StaticResource SubheaderTextStyle}"
                                                   Text="{TemplateBinding StatusMessage}"
                                                   core:DynamicStyleManager.SnappedStyle="{StaticResource SubheaderTextStyle_Snapped}" />
                                    </StackPanel>
                                    <Button Grid.Row="1"
                                            HorizontalAlignment="Center"
                                            Command="{TemplateBinding CancelCommand}"
                                            Content="Cancel"
                                            Style="{StaticResource CancelButton}" />
                                </Grid>
                            </Border>
                        </Border>
                        <Border HorizontalAlignment="Stretch"
                                VerticalAlignment="Stretch"
                                Background="{StaticResource LoadingPanelFullScreenBackground}"
                                Visibility="{TemplateBinding IsFailedVisibility}">
                            <Border Width="600"
                                    MinHeight="400"
                                    HorizontalAlignment="Center"
                                    VerticalAlignment="Center"
                                    Background="{StaticResource LoadingPanelBackground}"
                                    BorderBrush="#FF000000"
                                    BorderThickness="3">
                                <Grid Margin="20 10 20 10">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto" />
                                        <RowDefinition Height="*" />
                                        <RowDefinition Height="60" />
                                        <RowDefinition Height="50" />
                                    </Grid.RowDefinitions>
                                    <TextBlock Grid.Row="0"
                                               Margin="0 0 0 5"
                                               Style="{StaticResource SubheaderTextStyle}"
                                               Text="Error Ocurred" />
                                    <ScrollViewer Grid.Row="1"
                                                  ScrollViewer.HorizontalScrollBarVisibility="auto"
                                                  ScrollViewer.VerticalScrollBarVisibility="auto">
                                        <TextBox Height="Auto"
                                                 Background="#00FFFFFF"
                                                 ScrollViewer.HorizontalScrollBarVisibility="Auto"
                                                 ScrollViewer.VerticalScrollBarVisibility="Auto"
                                                 Style="{StaticResource ReadOnlyTextBox}"
                                                 Text="{Binding ErrorMessage,
                                                                Mode=OneWay}"
                                                 TextWrapping="Wrap" />
                                    </ScrollViewer>
                                    <StackPanel Grid.Row="2">
                                        <TextBlock Margin="0 5 0 3"
                                                   Style="{StaticResource StatusBarText}"
                                                   Text="Your Email for Response (optional)"
                                                   TextWrapping="Wrap" />
                                        <controls:PortableTextBox xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
                                                                  xmlns:p12="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                                  Style="{p12:StaticResource FormTextBoxStyle}"
                                                                  Text="{p12:Binding SendErrorCommandEmail,
                                                                                     UpdateSourceTrigger=LostFocus}" />
                                    </StackPanel>
                                    <StackPanel Grid.Row="3"
                                                HorizontalAlignment="Center"
                                                Orientation="Horizontal">
                                        <Button x:Name="PART_SendButton"
                                                Command="{Binding SendErrorCommand}"
                                                Content="Send Report"
                                                Style="{StaticResource CancelButton}" />
                                        <Button x:Name="PART_CloseButton"
                                                Content="Close"
                                                Style="{StaticResource OKButton}" />
                                    </StackPanel>
                                </Grid>
                            </Border>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           x:Key="{x:Type controls:PortablePopup}"
           TargetType="{x:Type controls:PortablePopup}">
