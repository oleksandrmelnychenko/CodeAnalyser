                    </StackPanel>
                </Border>
                <core:CustomDataTemplateSelector xmlns:p7="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                 HorizontalAlignment="Stretch"
                                                 HorizontalContentAlignment="Stretch"
                                                 VerticalContentAlignment="Stretch"
                                                 Content="{p7:Binding}"
                                                 Prefix="ChildListInFullSection"
                                                 p7:Grid.Row="1" />
            </Grid>
        </Border>
    </DataTemplate>
