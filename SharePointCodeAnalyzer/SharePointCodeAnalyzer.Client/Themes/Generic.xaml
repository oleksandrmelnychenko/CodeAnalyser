    <DataTemplate xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="ConfirmBoxTemplate">
        <Grid xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
              Panel.ZIndex="1000"
              core:StateManager.CurrentState="{Binding ConfirmBoxVisibility}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="ConfirmBoxStates">
                    <VisualState x:Name="HideConfirmBox">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.3"
                                             Storyboard.TargetName="confirmGrid"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="confirmGrid" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00.300">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="ShowConfirmBox">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.3"
                                             Storyboard.TargetName="confirmGrid"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="confirmGrid" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid x:Name="confirmGrid"
                  Background="{StaticResource ConfirmBoxFullScreenBackground}"
                  Opacity="0"
                  Visibility="Collapsed">
                <Grid.RenderTransform>
                    <TranslateTransform X="0" Y="0" />
                </Grid.RenderTransform>
                <Grid MaxHeight="480" Background="#FFE6E6F7">
                    <Border Height="96"
                            VerticalAlignment="Top"
                            Background="#FFD0DFEC" />
                    <Grid MaxWidth="800">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*" />
                            <RowDefinition Height="80" />
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0">
                            <core:PortableFrame xmlns:p10="http://schemas.microsoft.com/winfx/2006/xaml/presentation" Content="{p10:Binding CurrentConfirmBoxViewModel, Converter={p10:StaticResource ViewModelToViewConverter}}" />
                        </Border>
                        <Grid Grid.Row="1">
                            <StackPanel Margin="25 0 25 0"
                                        HorizontalAlignment="Right"
                                        Orientation="Horizontal">
                                <controls:FadingItemsControl xmlns:p11="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                             ItemTemplate="{p11:StaticResource AppCommandAsButtonItemTemplate}"
                                                             ItemsSource="{p11:Binding CurrentConfirmBoxViewModel.ContextCommands}"
                                                             Style="{p11:StaticResource StandardListBox}"
                                                             p11:Grid.Column="0">
                                    <controls:FadingItemsControl.ItemsPanel>
                                        <p11:ItemsPanelTemplate>
                                            <p11:StackPanel VerticalAlignment="Center" Orientation="Horizontal" />
                                        </p11:ItemsPanelTemplate>
                                    </controls:FadingItemsControl.ItemsPanel>
                                </controls:FadingItemsControl>
                                <StackPanel HorizontalAlignment="Right" Orientation="Horizontal">
                                    <Button Command="{Binding OnConfirmCancelCommand}"
                                            Content="Cancel"
                                            Style="{StaticResource CancelButton}" />
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>
        </Grid>
    </DataTemplate>
    <DataTemplate xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="WizardTemplate">
        <Grid xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
              Panel.ZIndex="100"
              core:StateManager.CurrentState="{Binding WizardVisibility}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="WizardStates">
                    <VisualState x:Name="HideWizard">
                        <Storyboard>
                            <DoubleAnimation BeginTime="0:0:0.1"
                                             Duration="0:0:0.3"
                                             Storyboard.TargetName="outerGrid"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="outerGrid" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00.500">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation BeginTime="0:0:0.1"
                                             Duration="0:0:0.3"
                                             Storyboard.TargetName="wizardGrid"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0" />
                            <DoubleAnimation Duration="0:0:0.3"
                                             From="1"
                                             Storyboard.TargetName="wizardGrid"
                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)"
                                             To="0.75">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseIn" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Duration="0:0:0.3"
                                             From="1"
                                             Storyboard.TargetName="wizardGrid"
                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleY)"
                                             To="0.75">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseIn" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="buttonPanel"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0" />
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="ShowWizard">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.2"
                                             Storyboard.TargetName="outerGrid"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="outerGrid" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0:0:0.2"
                                             Storyboard.TargetName="wizardGrid"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                            <DoubleAnimation Duration="0:0:0.3"
                                             From="0.5"
                                             Storyboard.TargetName="wizardGrid"
                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleX)"
                                             To="1">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Duration="0:0:0.3"
                                             From="0.5"
                                             Storyboard.TargetName="wizardGrid"
                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(ScaleTransform.ScaleY)"
                                             To="1">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                            <DoubleAnimation Duration="0"
                                             Storyboard.TargetName="buttonPanel"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border x:Name="outerGrid"
                    Background="{StaticResource LoadingPanelFullScreenBackground}"
                    Opacity="0"
                    Visibility="Collapsed">
                <Border x:Name="wizardGrid"
                        MaxWidth="900"
                        MaxHeight="700"
                        Margin="40"
                        BorderBrush="#FF000000"
                        BorderThickness="3"
                        DataContext="{Binding CurrentWizardViewModel}"
                        Opacity="0"
                        RenderTransformOrigin="0.5, 0.5">
                    <Border.RenderTransform>
                        <ScaleTransform ScaleX="1.0" ScaleY="1.0" />
                    </Border.RenderTransform>
                    <Grid Background="{StaticResource WizardBackground}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="auto" />
                            <RowDefinition Height="*" />
                            <RowDefinition Height="auto" />
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0"
                                Style="{StaticResource WizardBarBorderStyle}"
                                core:DynamicStyleManager.SnappedStyle="{StaticResource WizardBarBorderStyle_Snapped}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <StackPanel Grid.Column="0"
                                            VerticalAlignment="Top"
                                            Orientation="Vertical">
                                    <TextBlock Style="{StaticResource SectionHeaderTextStyle}"
                                               Text="{Binding DisplayName,
                                                              Converter={StaticResource StringToUpperCaseConverter}}"
                                               core:DynamicStyleManager.SnappedStyle="{StaticResource SectionHeaderTextStyle_Snapped}" />
                                    <TextBlock Style="{StaticResource SubheaderTextStyle}"
                                               Text="{Binding Description}"
                                               core:DynamicStyleManager.SnappedStyle="{StaticResource SubheaderTextStyle_Snapped}" />
                                    <Border Style="{StaticResource WizardStepListStyle}"
                                            Visibility="{Binding HasMultipleSteps,
                                                                 Converter={StaticResource BooleanToVisibilityConverter}}"
                                            core:DynamicStyleManager.SnappedStyle="{StaticResource WizardStepListStyle_Snapped}">
                                        <controls:FadingListView xmlns:p15="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                                 x:Name="root"
                                                                 HorizontalAlignment="Left"
                                                                 VerticalAlignment="Bottom"
                                                                 ItemContainerStyle="{p15:StaticResource ListBoxItemStyle1}"
                                                                 ItemTemplate="{p15:StaticResource wizardStepTemplate}"
                                                                 ItemsPanel="{p15:StaticResource horizontalStackPanelWithoutMargin}"
                                                                 ItemsSource="{p15:Binding Childs}"
                                                                 SelectedItem="{p15:Binding CurrentWizardStep,
                                                                                            Mode=TwoWay}"
                                                                 Style="{p15:StaticResource StandardListView}" />
                                    </Border>
                                </StackPanel>
                            </Grid>
                        </Border>
                        <core:PortableFrame xmlns:p9="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                            Content="{p9:Binding Converter={p9:StaticResource ViewModelToViewConverter}}"
                                            p9:Grid.Row="1" />
                        <Border Grid.Row="2"
                                Style="{StaticResource WizardAppBarStyle}"
                                core:DynamicStyleManager.SnappedStyle="{StaticResource WizardAppBarStyle_Snapped}">
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="auto" />
                                </Grid.ColumnDefinitions>
                                <controls:FadingItemsControl xmlns:p12="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                             ItemTemplate="{p12:StaticResource AppCommandItemTemplate}"
                                                             ItemsSource="{p12:Binding ContextCommands}"
                                                             Style="{p12:StaticResource StandardListBox}"
                                                             p12:Grid.Column="0">
                                    <controls:FadingItemsControl.ItemsPanel>
                                        <p12:ItemsPanelTemplate>
                                            <p12:StackPanel VerticalAlignment="Center" Orientation="Horizontal" />
                                        </p12:ItemsPanelTemplate>
                                    </controls:FadingItemsControl.ItemsPanel>
                                </controls:FadingItemsControl>
                                <StackPanel x:Name="buttonPanel"
                                            Grid.Column="1"
                                            HorizontalAlignment="Right"
                                            Orientation="Horizontal">
                                    <Button Command="{Binding GotoNextWizardStep}"
                                            Content="Next"
                                            Style="{StaticResource OKButton}"
                                            Visibility="{Binding HasNextStep,
                                                                 Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <Button Command="{Binding GotoNextWizardStep}"
                                            Content="Finish"
                                            Style="{StaticResource OKButton}"
                                            Visibility="{Binding HasFinishStep,
                                                                 Converter={StaticResource BooleanToVisibilityConverter}}" />
                                    <Button Command="{Binding CancelWizardCommand}"
                                            Content="Cancel"
                                            Style="{StaticResource CancelButton}" />
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>
                </Border>
            </Border>
        </Grid>
    </DataTemplate>
    <DataTemplate xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="DialogTemplate">
