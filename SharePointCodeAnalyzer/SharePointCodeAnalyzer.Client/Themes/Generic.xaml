                                                    <p12:Border Height="1"
                                                                Margin="20 3 20 3"
                                                                BorderBrush="#FF7A7A7A"
                                                                BorderThickness="1" />
                                                </p12:StackPanel>
                                            </p12:Border>
                                            <p12:Button Command="{p12:Binding GoHomeCommand}"
                                                        Content="Home"
                                                        Style="{p12:StaticResource MenuButton}" />
                                        </p12:StackPanel>
                                    </controls:PortablePopup>
                                </StackPanel>
                                <Button VerticalAlignment="Bottom"
                                        Command="{Binding OpenCommand}"
                                        DataContext="{Binding CurrentViewModel.ParentNavigationSection}"
                                        Style="{StaticResource TransparentButton}">
                                    <TextBlock Style="{StaticResource ParentNavigationSectionTextStyle}" Text="{Binding DisplayName}" />
                                </Button>
                            </StackPanel>
