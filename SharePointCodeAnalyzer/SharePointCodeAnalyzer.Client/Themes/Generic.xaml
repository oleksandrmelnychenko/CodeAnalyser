                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           x:Key="Fader"
           TargetType="{x:Type controls:FadingItemsControl}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type controls:FadingItemsControl}">
                    <Grid Background="#00FFFFFF">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="auto" />
                            <ColumnDefinition Width="*" />
                        </Grid.ColumnDefinitions>
                        <ListBox x:Name="root"
                                 Grid.Column="0"
                                 HorizontalAlignment="Left"
                                 Background="#FFFFFF00"
                                 ItemsSource="{Binding Items,
                                                       RelativeSource={RelativeSource TemplatedParent}}">
                            <ListBox.ItemsPanel>
                                <ItemsPanelTemplate>
                                    <StackPanel Margin="0 0 0 0" />
                                </ItemsPanelTemplate>
                            </ListBox.ItemsPanel>
                            <ListBox.ItemTemplate>
                                <DataTemplate>
                                    <Border Margin="5"
                                            Background="#00FFFFFF"
                                            BorderThickness="0">
                                        <TextBlock Text="{Binding DisplayName}" />
                                    </Border>
                                </DataTemplate>
                            </ListBox.ItemTemplate>
                        </ListBox>
                        <core:PortableFrame xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
                                            xmlns:p8="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                            Content="{p8:Binding SelectedItem,
                                                                 Converter={p8:StaticResource ViewModelToViewConverter},
                                                                 ElementName=root}"
                                            p8:Grid.Column="1" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
