                                        <SkewTransform />
                                        <RotateTransform />
                                        <TranslateTransform />
                                    </TransformGroup>
                                </StackPanel.RenderTransform>
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal">
