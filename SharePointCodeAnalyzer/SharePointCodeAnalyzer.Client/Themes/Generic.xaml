    <DataTemplate xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="MainFrameTemplate">
        <Grid xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
              Panel.ZIndex="0"
              core:StateManager.CurrentState="{Binding CurrentViewModel,
                                                       ConverterParameter=AnimateHeadline,
                                                       Converter={StaticResource PropertyChangeToStateChangeConverter}}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="MainStates">
                    <VisualState x:Name="AnimateHeadline">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.3"
                                             From="0"
                                             Storyboard.TargetName="header"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                            <DoubleAnimation Duration="0:0:0.3"
                                             From="50"
                                             Storyboard.TargetName="header"
                                             Storyboard.TargetProperty="(UIElement.RenderTransform).(TransformGroup.Children)[3].(TranslateTransform.X)"
                                             To="0">
                                <DoubleAnimation.EasingFunction>
                                    <CubicEase EasingMode="EaseOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid Background="{StaticResource BackgroundFullScreen}">
