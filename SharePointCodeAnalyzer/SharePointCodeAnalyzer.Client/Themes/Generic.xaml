                            <TextBlock Style="{StaticResource SectionHeaderTextStyle}" Text="{Binding DisplayName, Converter={StaticResource StringToUpperCaseConverter}}" />
                        </Button>
                        <StackPanel Margin="5 0 0 3" Orientation="Horizontal">
                            <TextBlock VerticalAlignment="Bottom"
                                       Style="{StaticResource SubheaderTextStyle}"
                                       Text="{Binding ItemCount}" />
                            <TextBlock VerticalAlignment="Bottom"
                                       Style="{StaticResource SubheaderTextStyle}"
                                       Text=" Items" />
