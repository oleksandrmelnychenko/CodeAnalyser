                                                                BorderBrush="#FF7A7A7A"
                                                                BorderThickness="1" />
                                                </p12:StackPanel>
                                            </p12:Border>
                                            <p12:Border Visibility="{p12:Binding CurrentViewModel.HasSiblings, Converter={p12:StaticResource BooleanToVisibilityConverter}}">
                                                <p12:StackPanel>
                                                    <controls:FadingItemsControl xmlns:p17="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                                                 DataContext="{p17:Binding CurrentViewModel}"
                                                                                 ItemsSource="{p17:Binding Siblings}">
                                                        <controls:FadingItemsControl.ItemsPanel>
