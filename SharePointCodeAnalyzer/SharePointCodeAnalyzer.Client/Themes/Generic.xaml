    <DataTemplate xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" x:Key="TileTemplate_BaseItemViewModel">
        <Button xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
                x:Name="border"
                HorizontalAlignment="Left"
                Command="{Binding OpenCommand}"
                IsEnabled="{Binding IsEnabled}"
                Style="{StaticResource ItemTile}"
                core:StateManager.CurrentState="{Binding FilterState}">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="FilterStates">
                    <VisualState x:Name="NotFilteredOut">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="border"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="1" />
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="border"
                                             Storyboard.TargetProperty="(FrameworkElement.Height)"
                                             To="120">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseInOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="FilteredOut">
                        <Storyboard>
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="border"
                                             Storyboard.TargetProperty="(UIElement.Opacity)"
                                             To="0" />
                            <DoubleAnimation Duration="0:0:0.4"
                                             Storyboard.TargetName="border"
                                             Storyboard.TargetProperty="(FrameworkElement.Height)"
                                             To="0">
                                <DoubleAnimation.EasingFunction>
                                    <QuarticEase EasingMode="EaseInOut" />
                                </DoubleAnimation.EasingFunction>
                            </DoubleAnimation>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="auto" />
                    <ColumnDefinition Width="*" />
                </Grid.ColumnDefinitions>
                <Border Grid.Column="0" VerticalAlignment="Top">
                    <core:CustomDataTemplateSelector xmlns:p8="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                     Content="{p8:Binding}"
                                                     Prefix="ItemIcon" />
                </Border>
                <StackPanel Grid.Column="1" VerticalAlignment="Top">
                    <TextBlock Style="{StaticResource ItemHeadline}"
                               Text="{Binding DisplayName}"
                               TextWrapping="Wrap" />
                    <TextBlock Opacity="0.5"
                               Style="{StaticResource PropertyValue}"
                               Text="{Binding Description}"
                               TextWrapping="Wrap" />
                    <ItemsControl ItemsSource="{Binding PreviewProperties}">
                        <ItemsControl.ItemsPanel>
                            <ItemsPanelTemplate>
                                <controls:SimpleWrapPanel xmlns:p11="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                          VerticalAlignment="Center"
                                                          Orientation="Vertical" />
                            </ItemsPanelTemplate>
                        </ItemsControl.ItemsPanel>
                        <ItemsControl.ItemTemplate>
                            <DataTemplate>
                                <StackPanel Orientation="Horizontal">
                                    <TextBlock Opacity="0.5"
                                               Style="{StaticResource PropertyTitle}"
                                               Text="{Binding PropertyDisplayName}" />
                                    <TextBlock Margin="5 0 15 0"
                                               Opacity="0.75"
                                               Style="{StaticResource PropertyValue}"
                                               Text="{Binding PropertyValue}" />
                                </StackPanel>
                            </DataTemplate>
                        </ItemsControl.ItemTemplate>
                    </ItemsControl>
                </StackPanel>
            </Grid>
        </Button>
    </DataTemplate>
