    <Style xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
           x:Key="{x:Type controls:SimpleDataGrid}"
           TargetType="{x:Type controls:SimpleDataGrid}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type controls:SimpleDataGrid}">
                    <Border>
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="*" />
                            </Grid.RowDefinitions>
                            <Border Grid.Row="0" Background="{Binding HeaderBackground, RelativeSource={RelativeSource Mode=TemplatedParent}}">
                                <ItemsControl x:Name="PART_Headers" ItemsSource="{Binding Headers, RelativeSource={RelativeSource Mode=TemplatedParent}}">
                                    <ItemsControl.ItemsPanel>
                                        <ItemsPanelTemplate>
                                            <controls:SimpleDataGridRowPanel />
                                        </ItemsPanelTemplate>
                                    </ItemsControl.ItemsPanel>
                                </ItemsControl>
                            </Border>
                            <controls:FadingListView xmlns:local="http://vidoori.com/VOXGAP.Client.Core"
                                                     xmlns:p9="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                                                     x:Name="PART_GridArea"
                                                     Background="#00FFFFFF"
                                                     ItemContainerStyle="{p9:StaticResource StandardListBoxItem}"
                                                     ItemsSource="{p9:Binding Rows,
                                                                              RelativeSource={p9:RelativeSource Mode=TemplatedParent}}"
                                                     p9:Grid.Row="1">
                                <controls:FadingListView.ItemsPanel>
                                    <p9:ItemsPanelTemplate>
                                        <p9:VirtualizingStackPanel p9:ScrollViewer.VerticalScrollBarVisibility="Visible" />
                                    </p9:ItemsPanelTemplate>
                                </controls:FadingListView.ItemsPanel>
                                <controls:FadingListView.Template>
                                    <p9:ControlTemplate>
                                        <p9:Border x:Name="LayoutRoot"
                                                   Background="{p9:TemplateBinding Background}"
                                                   BorderBrush="#00FFFFFF"
                                                   BorderThickness="0">
                                            <p9:ScrollViewer x:Name="ScrollViewer"
                                                             BorderBrush="#00FFFFFF"
                                                             BorderThickness="0"
                                                             Padding="{p9:TemplateBinding Padding}"
                                                             p9:ScrollViewer.HorizontalScrollBarVisibility="{p9:TemplateBinding p9:ScrollViewer.HorizontalScrollBarVisibility}"
                                                             p9:ScrollViewer.VerticalScrollBarVisibility="{p9:TemplateBinding p9:ScrollViewer.VerticalScrollBarVisibility}">
                                                <p9:ItemsPresenter />
                                            </p9:ScrollViewer>
                                        </p9:Border>
                                    </p9:ControlTemplate>
                                </controls:FadingListView.Template>
                                <controls:FadingListView.ItemTemplate>
                                    <p9:DataTemplate>
                                        <p9:Button x:Name="border"
                                                   Height="{p9:Binding RowHeight}"
                                                   Command="{p9:Binding ReferencedItem.OpenCommand}"
                                                   Style="{p9:StaticResource ClickableDataRow}"
                                                   core:StateManager.CurrentState="{p9:Binding ReferencedItem.FilterState}">
                                            <p9:VisualStateManager.VisualStateGroups>
                                                <p9:VisualStateGroup x:Name="FilterStates">
                                                    <p9:VisualState x:Name="NotFilteredOut">
                                                        <p9:Storyboard>
                                                            <p9:DoubleAnimation Duration="0:0:0.4"
                                                                                To="1"
                                                                                p9:Storyboard.TargetName="border"
                                                                                p9:Storyboard.TargetProperty="(p9:UIElement.Opacity)" />
                                                            <p9:DoubleAnimation Duration="0:0:0.4"
                                                                                To="{p9:Binding RowHeight}"
                                                                                p9:Storyboard.TargetName="border"
                                                                                p9:Storyboard.TargetProperty="(p9:FrameworkElement.Height)">
                                                                <p9:DoubleAnimation.EasingFunction>
                                                                    <p9:QuarticEase EasingMode="EaseInOut" />
                                                                </p9:DoubleAnimation.EasingFunction>
                                                            </p9:DoubleAnimation>
                                                        </p9:Storyboard>
                                                    </p9:VisualState>
                                                    <p9:VisualState x:Name="FilteredOut">
                                                        <p9:Storyboard>
                                                            <p9:DoubleAnimation Duration="0:0:0.4"
                                                                                To="0"
                                                                                p9:Storyboard.TargetName="border"
                                                                                p9:Storyboard.TargetProperty="(p9:UIElement.Opacity)" />
                                                            <p9:DoubleAnimation Duration="0:0:0.4"
                                                                                To="0"
                                                                                p9:Storyboard.TargetName="border"
                                                                                p9:Storyboard.TargetProperty="(p9:FrameworkElement.Height)">
                                                                <p9:DoubleAnimation.EasingFunction>
                                                                    <p9:QuarticEase EasingMode="EaseInOut" />
                                                                </p9:DoubleAnimation.EasingFunction>
                                                            </p9:DoubleAnimation>
                                                        </p9:Storyboard>
                                                    </p9:VisualState>
                                                </p9:VisualStateGroup>
                                            </p9:VisualStateManager.VisualStateGroups>
                                            <p9:ItemsControl x:Name="PART_Rows" ItemsSource="{p9:Binding Cells}">
                                                <p9:ItemsControl.ItemsPanel>
                                                    <p9:ItemsPanelTemplate>
                                                        <controls:SimpleDataGridRowPanel xmlns:p18="http://schemas.microsoft.com/winfx/2006/xaml/presentation" Background="{p18:Binding Background}" />
                                                    </p9:ItemsPanelTemplate>
                                                </p9:ItemsControl.ItemsPanel>
                                            </p9:ItemsControl>
                                        </p9:Button>
                                    </p9:DataTemplate>
                                </controls:FadingListView.ItemTemplate>
                            </controls:FadingListView>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
